{
  "name": "expensify-app-mock",
  "version": "1.0.0",
  "description": "Mock repository for testing octocov implementation",
  "main": "src/App.js",
  "scripts": {
    "test": "TZ=utc NODE_OPTIONS=--experimental-vm-modules jest",
    "test:coverage": "TZ=utc NODE_OPTIONS=--experimental-vm-modules jest --coverage --coverageReporters=lcov,text",
    "test:ci": "TZ=utc NODE_OPTIONS=--experimental-vm-modules jest --coverage --ci --watchAll=false --coverageReporters=lcov,text",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build": "react-native build"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-native": "^0.72.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-flow": "^7.27.1",
    "@babel/preset-react": "^7.22.5",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "react-test-renderer": "^18.2.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.(test|spec).js",
      "<rootDir>/src/**/__tests__/**/*.(test|spec).js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.stories.js",
      "!src/index.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html",
      "json"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native)/)"
    ],
    "testTimeout": 10000
  }
}
